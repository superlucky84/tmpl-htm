{
  "name": "tmpl-htm",
  "version": "1.0.1",
  "description": "An extensible virtual DOM library for lightweight use in a variety of environments.",
  "packageManager": "pnpm@7.6.0",
  "main": "./dist/tmplHtm.umd.js",
  "module": "./dist/tmplHtm.mjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run --reporter verbose",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "pnpm clean:dist && pnpm clean:module",
    "clean:dist": "rm -rf dist",
    "clean:module": "rm -rf node_modules"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tmplHtm.mjs",
      "require": "./dist/tmplHtm.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/superlucky84/tmpl-htm.git"
  },
  "homepage": "https://github.com/superlucky84/tmpl-htm",
  "license": "MIT",
  "keywords": [
    "tmpl",
    "tmpl-htm",
    "template"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@nabla/vite-plugin-eslint": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitest/coverage-v8": "^0.32.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "happy-dom": "^9.20.3",
    "jsdom": "^22.1.0",
    "prettier": "^2.3.2",
    "terser": "^5.17.7",
    "typescript": "^4.7.4",
    "vite": "3.2.7",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.2"
  },
  "volta": {
    "node": "20.3.0"
  },
  "dependencies": {
    "htm": "^3.1.1"
  }
}
